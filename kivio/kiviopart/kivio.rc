  <Action name="---"/>
  <Action name="setHTextAlignment"/>
  <Action name="setVTextAlignment"/>
</ToolBar>
<ToolBar name="ToolsToolBar" fullWidth="false" position="left" >
</ToolBar>
</kpartgui>
