<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>StencilsBarOptionsDialogBase</class>
<include location="global">kcolorbutton.h</include>
<widget class="QWidget">
    <property name="name">
        <cstring>StencilsBarOptionsDialogBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>505</width>
            <height>409</height>
        </rect>
    </property>
    <property name="caption">
        <string></string>
    </property>
    <property name="layoutSpacing" stdset="0">
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QLineEdit" row="1" column="1" rowspan="1" colspan="2">
            <property name="name">
                <cstring>pixmapName</cstring>
            </property>
        </widget>
        <widget class="QPushButton" row="1" column="3">
            <property name="name">
                <cstring>openButton</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>0</hsizetype>
                    <vsizetype>0</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string></string>
            </property>
            <property name="pixmap">
                <pixmap>"fileopen"</pixmap>
            </property>
        </widget>
        <widget class="QRadioButton" row="1" column="0">
            <property name="name">
                <cstring>usePixmap</cstring>
            </property>
            <property name="text">
                <string>Background &amp;pixmap:</string>
            </property>
        </widget>
        <widget class="QLabel" row="2" column="0" rowspan="1" colspan="4">
            <property name="name">
                <cstring>preview</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>7</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="pixmap">
                <pixmap>""</pixmap>
            </property>
            <property name="alignment">
                <set>AlignTop|AlignLeft</set>
            </property>
            <property name="vAlign" stdset="0">
            </property>
        </widget>
        <widget class="QRadioButton" row="0" column="0">
            <property name="name">
                <cstring>useColor</cstring>
            </property>
            <property name="text">
                <string>&amp;Background color:</string>
            </property>
        </widget>
        <widget class="KColorButton" row="0" column="1">
            <property name="name">
                <cstring>color</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>1</hsizetype>
                    <vsizetype>1</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string></string>
            </property>
        </widget>
        <spacer row="0" column="2">
            <property name="name">
                <cstring>Spacer1</cstring>
            </property>
            <property name="orientation">
                <enum>Horizontal</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
        </spacer>
    </grid>
</widget>
<connections>
    <connection>
        <sender>openButton</sender>
        <signal>clicked()</signal>
        <receiver>StencilsBarOptionsDialogBase</receiver>
        <slot>slotOpenButton()</slot>
    </connection>
    <connection>
        <sender>useColor</sender>
        <signal>clicked()</signal>
        <receiver>StencilsBarOptionsDialogBase</receiver>
        <slot>selectSource()</slot>
    </connection>
    <connection>
        <sender>usePixmap</sender>
        <signal>clicked()</signal>
        <receiver>StencilsBarOptionsDialogBase</receiver>
        <slot>selectSource()</slot>
    </connection>
    <connection>
        <sender>pixmapName</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>StencilsBarOptionsDialogBase</receiver>
        <slot>updatePreview()</slot>
    </connection>
</connections>
<includes>
    <include location="global" impldecl="in declaration">kiconloader.h</include>
</includes>
<slots>
    <slot access="protected">updatePreview()</slot>
    <slot access="protected">selectSource()</slot>
    <slot access="protected">slotOpenButton()</slot>
</slots>
<pixmapfunction>BarIcon</pixmapfunction>
<layoutdefaults spacing="6" margin="11"/>
</UI>
