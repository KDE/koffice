	<Syntax>ROUNDUP(value;[digits])</Syntax>
	<Example>ROUNDUP(1.252;2) equals 1.26</Example>
	<Example>ROUNDUP(-1.252;2) equals -1.25</Example>
	<Example>ROUNDUP(-1.252) equals -1</Example>
      </Help>
