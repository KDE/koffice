    <Function>
      <Name>SIGN</Name>
      <Type>Int</Type>
      <Parameter>
	<Comment>Floating point value</Comment>
	<Type>Float</Type>
      </Parameter>
      <Help>
	<Text>This function returns -1 if the number is negative, 0 if the number is null and 1 if the number is positive.</Text>
	<Syntax>SIGN(value)</Syntax>
