# Set which toolbars sholuld be hidden/visible per default.
[DockerManager]
HiddenToolDockers=
VisibleToolDockers=KSpreadCellToolId,DefaultToolArrangeWidget,DefaultToolWidget,SnapGuideConfigWidget,TableToolFactoryId

# Set the default position of the KSpreadCellToolId to the top area. This follows the
# default Calc/Excel like style to have the inout field on top.
[kspread][DockWidget KSpreadCellToolId]
Collapsed=false
DockArea=4

# Per default remove all dockers except the KSpreadCellToolId one.
[kspread]

