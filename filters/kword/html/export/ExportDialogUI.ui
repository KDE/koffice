<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>ExportDialogUI</class>
<comment>Export dialog for KWord's HTML export filter</comment>
<author>Nicolas GOUTTE &lt;nicog@snafu.de&gt;</author>
<widget class="QWidget">
    <property name="name">
        <cstring>ExportDialogUI</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>254</width>
            <height>291</height>
        </rect>
    </property>
    <property name="caption">
        <string>HTML Export Dialog</string>
    </property>
    <widget class="QLayoutWidget">
        <property name="name">
            <cstring>Layout2</cstring>
        </property>
        <property name="geometry">
            <rect>
                <x>10</x>
                <y>11</y>
                <width>202</width>
                <height>240</height>
            </rect>
        </property>
        <vbox>
            <property name="name">
                <cstring>unnamed</cstring>
            </property>
            <property name="margin">
                <number>0</number>
            </property>
            <property name="spacing">
                <number>6</number>
            </property>
            <widget class="QButtonGroup">
                <property name="name">
                    <cstring>buttonGroup1</cstring>
                </property>
                <property name="title">
                    <string>Document Type</string>
                </property>
                <vbox>
                    <property name="name">
                        <cstring>unnamed</cstring>
                    </property>
                    <property name="margin">
                        <number>11</number>
                    </property>
                    <property name="spacing">
                        <number>6</number>
                    </property>
                    <widget class="QRadioButton">
                        <property name="name">
                            <cstring>radioDocType1</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;HTML 4.01</string>
                        </property>
                        <property name="checked">
                            <bool>false</bool>
                        </property>
                    </widget>
                    <widget class="QRadioButton">
                        <property name="name">
                            <cstring>radioDocType2</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;XHTML 1.0</string>
                        </property>
                        <property name="checked">
                            <bool>true</bool>
                        </property>
                    </widget>
                </vbox>
            </widget>
            <widget class="QButtonGroup">
                <property name="name">
                    <cstring>buttonGroupEncoding</cstring>
                </property>
                <property name="title">
                    <string>Encoding</string>
                </property>
                <vbox>
                    <property name="name">
                        <cstring>unnamed</cstring>
                    </property>
                    <property name="margin">
                        <number>11</number>
                    </property>
                    <property name="spacing">
                        <number>6</number>
                    </property>
                    <widget class="QRadioButton">
                        <property name="name">
                            <cstring>radioEncodingUTF8</cstring>
                        </property>
                        <property name="text">
                            <string>UTF-&amp;8</string>
                        </property>
                        <property name="checked">
                            <bool>true</bool>
                        </property>
                    </widget>
                    <widget class="QRadioButton">
                        <property name="name">
                            <cstring>radioEncodingLocal</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;Local Encoding</string>
                        </property>
                        <property name="checked">
                            <bool>false</bool>
                        </property>
                    </widget>
                    <widget class="QRadioButton">
                        <property name="name">
                            <cstring>radioEncodingOther</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;Other Encoding:</string>
                        </property>
                        <property name="checked">
                            <bool>false</bool>
                        </property>
                        <property name="buttonGroupId">
                            <number>2</number>
                        </property>
                    </widget>
                    <widget class="QComboBox">
                        <property name="name">
                            <cstring>comboBoxEncoding</cstring>
                        </property>
                        <property name="sizePolicy">
                            <sizepolicy>
                                <hsizetype>1</hsizetype>
                                <vsizetype>5</vsizetype>
                                <horstretch>0</horstretch>
                                <verstretch>0</verstretch>
                            </sizepolicy>
                        </property>
                    </widget>
                </vbox>
            </widget>
            <widget class="QCheckBox">
                <property name="name">
                    <cstring>CheckBoxAuthorInfo</cstring>
                </property>
                <property name="enabled">
                    <bool>false</bool>
                </property>
                <property name="text">
                    <string>&amp;Export information about author</string>
                </property>
            </widget>
        </vbox>
    </widget>
</widget>
<tabstops>
    <tabstop>radioDocType1</tabstop>
    <tabstop>radioDocType2</tabstop>
</tabstops>
<layoutdefaults spacing="6" margin="11"/>
</UI>
