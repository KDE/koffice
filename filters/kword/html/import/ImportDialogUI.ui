<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>ImportDialogUI</class>
<comment>Dialog for the HTML import of KWord</comment>
<author>Nicolas GOUTTE &lt;nicog@snafu.de&gt;</author>
<widget class="QWidget">
    <property name="name">
        <cstring>ImportDialogUI</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>380</width>
            <height>216</height>
        </rect>
    </property>
    <property name="caption">
        <string>HTML Import Dialog</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>groupCharSet</cstring>
            </property>
            <property name="title">
                <string>Character Set</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>textLabelCharset</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Hint for character set:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>lineEditCharSet</cstring>
                    </property>
                </widget>
                <widget class="QLineEdit">
                    <property name="name">
                        <cstring>lineEditCharSet</cstring>
                    </property>
                </widget>
            </vbox>
        </widget>
        <widget class="QButtonGroup">
            <property name="name">
                <cstring>buttonHint</cstring>
            </property>
            <property name="title">
                <string>&amp;Hint</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>radioButtonHintNone</cstring>
                    </property>
                    <property name="text">
                        <string>Do &amp;not use the hint. (Recommended!)</string>
                    </property>
                    <property name="checked">
                        <bool>true</bool>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>radioButtonHintFallback</cstring>
                    </property>
                    <property name="text">
                        <string>Use hint only as &amp;fall back.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>radioButtonHintForce</cstring>
                    </property>
                    <property name="text">
                        <string>Force the use of the hint. (For buggy files)</string>
                    </property>
                </widget>
            </vbox>
        </widget>
        <spacer>
            <property name="name" stdset="0">
                <cstring>Spacer58</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>20</width>
                    <height>20</height>
                </size>
            </property>
        </spacer>
    </vbox>
</widget>
<layoutdefaults spacing="6" margin="11"/>
</UI>
