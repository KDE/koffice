<PAPER>
	- format			SUPORTED
	- ptWidth			SUPORTED
	- ptHeight			SUPORTED
	- mmWidth ="210"		SUPORTED
	- mmHeight="297"		SUPORTED
	- inchWidth ="8.26772"		SUPORTED
	- inchHeight="11.6929"		SUPORTED
	- orientation			SUPORTED
	- columns="1"			SUPORTED
	- ptColumnspc="3"		ANALIZED
	- mmColumnspc="1.05833"		ANALIZED
	- inchColumnspc="0.0416667"	ANALIZED
	- hType="0"			SUPORTED
		0...On all pages (even/odd) the same headers	\chead
		1...Different header only on first page
		2...Different headers for even/odd pages	\fancyhead
	- fType="0"			SUPORTED
	- ptHeadBody="9"		SUPORTED
	- ptFootBody="9"		SUPORTED
	- mmHeadBody="3.5"		SUPORTED
	- mmFootBody="3.5"		SUPORTED
	- inchHeadBody="0.137795"	SUPORTED
	- inchFootBody="0.137795"	SUPORTED

	<PAPERBORDERS>			ALL SUPORTED EXCEPT FOR BOTTOM (don't exist)
					SPECIAL USE FOR Right and Left.
	- mmLeft="0"
	- mmTop="0"
	- mmRight="0"
	- mmBottom="0"
	- ptLeft="0"
	- ptTop="0"
	- ptRight="0"
	- ptBottom="0"
	- inchLeft="0"
	- inchTop="0"
	- inchRight="0"
	- inchBottom="0"

