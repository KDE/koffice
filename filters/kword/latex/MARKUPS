<PARAGRAPH>
	- <TEXT>		SUPORTED :))
	- <NAME>		ANLYSED
		- name="Footnote/Endnote_1"
	<INFO>
		- info="0"
			- 0...No "special" information
			- 1...Footnote (see <NAME>)
	<HARDBRK>
		- frame="0"
			- 0...let it flow
			- 1...hard brake -> next frame

	<FORMATS>
		- <FORMAT>
			- id="1"	ANALYSED
				- 0...none (mustn't be in a file)
				- 1..."normal" text
				- 2...a picture
				- 3...tabulator
				- 4...a variable
				- 5...a footnote
			- pos="0"
			- len="5"

   		- <FORMAT id="1" ...> texte
			COLOR			SUPORTED
			FONT			ANALYSED
			SIZE			SUPORTED
			WEIGHT			SUPORTED
			ITALIC			SUPORTED
			UNDERLINE		SUPORTED
			VERTALIGN		SUPORTED

		- <FORMAT id="2"...>
			- <FILENAME value="/home/..."/>

		- <FORMAT id="3"...>

		- <FORMAT id="4"...>
			- <TYPE type="0"/>
				- 0...date (fix)
				- 1...date (variable)
				- 2...time (fix)
				- 3...time (variable)
				- 4...page number
			- <POS frameSet="1" frame="1" pageNum="1"/>

			- <DATE year="1999" month="8" day="22" fix="1"/>
			- <FRMAT>
			- <COLOR red="0" green="0" blue="0"/>
			- <FONT name="times"/>
			- <SIZE value="12"/>
			- <WEIGHT value="50"/>
			- <ITALIC value="0"/>
			- <UNDERLINE value="0"/>
			- <VERTALIGN value="0"/>
			- </FRMAT>

		- <FORMAT id="5"...>	ANALYSED
			- <INTERNAL>
			- <PART from="1" to="-1" space="-"/>
			- </INTERNAL>
			- <RANGE start="1" end="1"/>
			- <TEXT before="[ " after=" ]"/>
			- <DESCRIPT ref="Footnote/Endnote_1"/>
			- <FRMAT>
			- <COLOR red="0" green="0" blue="0"/>
			- <FONT name="times"/>
			- <SIZE value="12"/>
			- <WEIGHT value="50"/>
			- <ITALIC value="0"/>
			- <UNDERLINE value="0"/>
			- <VERTALIGN value="2"/>
			- </FRMAT>

<LAYOUT>
	- <NAME>
		- value="Standard"	SUPORTED
	- <FOLLOWING>
		- name="Standard"	SUPORTED
