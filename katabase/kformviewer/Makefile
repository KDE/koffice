	rm -f kformviewer.all_cc.files kformviewer.all_cc.final; \
	echo "#define KDE_USE_FINAL 1" >> kformviewer.all_cc.final; \
	for file in kformviewer_main.cc kformviewer_shell.cc kformviewer_doc.cc kformviewer_view.cc kformviewer.cc ; do \
	  echo "#include \"$$file\"" >> kformviewer.all_cc.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> kformviewer.all_cc.final; \
	done; \
	cat kformviewer.all_cc.final kformviewer.all_cc.files  > kformviewer.all_cc.cc; \
	rm -f kformviewer.all_cc.final kformviewer.all_cc.files

#>+ 3
clean-final:
	-rm -f kformviewer.all_cc.cc

#>+ 2
final:
	$(MAKE) kformviewer_OBJECTS="$(kformviewer_final_OBJECTS)" all-am
#>+ 3
cvs-clean:
	$(MAKE) -f $(top_srcdir)/admin/Makefile.common cvs-clean
